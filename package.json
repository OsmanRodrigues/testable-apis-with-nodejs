    "babel-eslint": "^10.1.0",
    "chai": "^4.4.1",
    "eslint": "^6.8.0",
    "eslint-plugin-node": "^10.0.0",
    "mocha": "^6.2.3",
    "prettier": "^1.19.1",
    "sinon": "^7.5.0",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "bcrypt": "^3.0.8",
    "body-parser": "^1.20.2",
    "config": "^1.29.4",
    "express": "^4.19.2",
